{
    "QQLogin": {
        "compiler": "g++",
        "src": [
            "./src/*.cpp",
            "./main.cpp"
        ],
        "include": [
            "./include"
        ],
        "link": [
            "egrome",
            "graphics64",
            "gdiplus",
            "uuid",
            "msimg32",
            "gdi32",
            "imm32",
            "ole32",
            "oleaut32",
            "winmm"
        ],
        "compile": true,
        "start": true,
        "target": "./QQLogin.exe",
        "option": [
            "-O4"
        ]
        // "define": [
        //     "UNICODE",
        //     "_UNICODE"
        // ]
    }
}